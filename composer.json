{
    "name":"superlogin/contao-client",
    "description":"SuperLogin Client for Contao 5",
    "keywords":["contao","login"],
    "type":"contao-bundle",
    "homepage":"https://github.com/superlogin/contao-client",
    "license":"proprietary",
    "authors":[{
        "name": "Hendrik Obermayer",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4",
        "league/oauth2-client": "^2.7",
        "contao/core-bundle": "^5.3",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/security-bundle": "^5.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^3.0"
    },
    "autoload":{
        "psr-4": {
            "Comolo\\SuperLoginClient\\ContaoEdition\\": "src/"
        }
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "Comolo\\SuperLoginClient\\ContaoEdition\\ContaoManager\\Plugin"
    }
}
